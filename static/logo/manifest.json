{
  "manifest_version": 2,
  "name": "MyBlog",
  "short_name": "MyBlog",
  "version": "versionString",
  "default_locale": "en",
  "description": "MyBlog Hugo Theme",
  "icons": [
    {
      "src": "logo/favicon.png",
      "sizes": "16x16",
      "type": "image/png"
    }, {
      "src": "logo/logo.png",
      "sizes": "48x48",
      "type": "image/png"
    }, {
      "src": "logo/logo-192.png",
      "sizes": "192x192",
      "type": "image/png"
    }, {
      "src": "logo/logo-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "background": {
    "persistent": false
  },
  "start_url": "https://example.com/",
  "theme_color": "#fff",
  "background_color": "#fff",
  "display": "standalone"
}
